{
  "name": "@sandboxes/modernjs-react-mf-remote",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build": "modern build",
    "dev": "modern dev",
    "lint": "biome check",
    "new": "modern new",
    "prepare": "simple-git-hooks",
    "reset": "npx rimraf node_modules ./**/node_modules",
    "serve": "modern serve",
    "start": "modern start",
    "upgrade": "modern upgrade"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "@modern-js/runtime": "2.68.11",
    "@module-federation/modern-js": "0.18.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@modern-js/app-tools": "^2.68.11",
    "@modern-js/tsconfig": "2.68.11",
    "@types/node": "^18.19.110",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "rimraf": "^6.0.1",
    "ts-node": "~10.9.2",
    "tsconfig-paths": "~3.15.0",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.18.1"
  }
}
