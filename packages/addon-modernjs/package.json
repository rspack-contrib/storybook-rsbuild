{
  "name": "storybook-addon-modernjs",
  "version": "3.1.0",
  "description": "Storybook addon for loading configuration of Modern.js",
  "keywords": [
    "storybook",
    "storybook-addons",
    "rsbuild",
    "rspack",
    "modernjs"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/addon-modernjs"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./preset": "./dist/preset.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@modern-js/plugin-v2": "^2.69.2",
    "find-up": "^5.0.0",
    "rslog": "1.3.2"
  },
  "devDependencies": {
    "@modern-js/app-tools": "^2.69.2",
    "@rsbuild/core": "^1.6.12",
    "@types/node": "^22.0.0",
    "storybook": "10.1.2",
    "storybook-builder-rsbuild": "workspace:*",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@modern-js/app-tools": "^2.67.9",
    "@rsbuild/core": "^1.5.0",
    "storybook-builder-rsbuild": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
