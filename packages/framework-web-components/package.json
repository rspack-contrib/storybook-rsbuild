{
  "name": "storybook-web-components-rsbuild",
  "version": "3.2.0",
  "description": "Storybook for web component and Rsbuild: Develop React Component in isolation with Hot Reloading.",
  "keywords": [
    "storybook",
    "rsbuild",
    "rspack",
    "web component",
    "lit",
    "lit2",
    "lit3"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/framework-web-components"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./package.json": "./package.json",
    "./preset": "./dist/preset.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@storybook/web-components": "^10.1.10",
    "storybook-builder-rsbuild": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.13",
    "@types/node": "^22.0.0",
    "lit": "^3.3.1",
    "storybook": "10.1.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.5.0",
    "lit": "^2.0.0 || ^3.0.0",
    "storybook": "^10.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/node/index.ts",
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
