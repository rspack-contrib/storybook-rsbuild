{
  "name": "storybook-builder-rsbuild",
  "version": "3.2.0",
  "description": "Rsbuild builder for Storybook",
  "keywords": [
    "storybook",
    "rsbuild",
    "rspack"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/builder-rsbuild"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./loaders/export-order-loader": "./dist/loaders/export-order-loader.js",
    "./loaders/rsbuild-automock-loader": "./dist/loaders/rsbuild-automock-loader.js",
    "./loaders/storybook-mock-transform-loader": "./dist/loaders/storybook-mock-transform-loader.js",
    "./package.json": "./package.json",
    "./presets/preview-preset": "./dist/preview-preset.js",
    "./templates/preview.ejs": "./templates/preview.ejs",
    "./templates/virtualModuleEntry.template.js": "./templates/virtualModuleEntry.template.js",
    "./templates/virtualModuleModernEntry.js": "./templates/virtualModuleModernEntry.js",
    "./templates/virtualModuleStory.template.js": "./templates/virtualModuleStory.template.js"
  },
  "files": [
    "dist/**/*",
    "compiled/**/*",
    "templates/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "prebundle && jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@rsbuild/plugin-type-check": "^1.3.2",
    "@vitest/mocker": "3.2.4",
    "browser-assert": "^1.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "cjs-module-lexer": "^2.1.1",
    "constants-browserify": "^1.0.0",
    "es-module-lexer": "^1.7.0",
    "fs-extra": "^11.3.2",
    "magic-string": "^0.30.21",
    "path-browserify": "^1.0.1",
    "picocolors": "^1.1.1",
    "process": "^0.11.10",
    "rsbuild-plugin-html-minifier-terser": "^1.1.2",
    "sirv": "^2.0.4",
    "ts-dedent": "^2.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "util-deprecate": "^1.0.2"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.13",
    "@storybook/core-webpack": "10.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "@types/pretty-hrtime": "^1.0.3",
    "prebundle": "^1.6.0",
    "pretty-hrtime": "^1.0.3",
    "slash": "^5.1.0",
    "storybook": "10.1.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.5.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "storybook": "^10.1.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preview-preset.ts",
      "./src/loaders/export-order-loader.ts",
      "./src/loaders/storybook-mock-transform-loader.ts",
      "./src/loaders/rsbuild-automock-loader.ts"
    ],
    "platform": "node"
  }
}
