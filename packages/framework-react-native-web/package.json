{
  "name": "storybook-react-native-web-rsbuild",
  "version": "0.0.0",
  "description": "Storybook for React Native Web and Rsbuild: build React Native components for the web with Storybook.",
  "keywords": [
    "storybook",
    "rsbuild",
    "react-native-web",
    "react-native"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/framework-react-native-web"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "require": "./dist/preset.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "node": "./dist/node/index.js",
      "import": "./dist/node/index.mjs",
      "require": "./dist/node/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --optimized",
    "check": "jiti ../../scripts/prepare/check.ts",
    "prep": "jiti ../../scripts/prepare/bundle.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@storybook/react": "^9.1.8",
    "babel-loader": "^9.2.1",
    "storybook-builder-rsbuild": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.11",
    "@types/node": "^18.19.110",
    "@types/react": "^18.3.24",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "9.1.8",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-react": "*",
    "@rsbuild/core": "^1.5.0",
    "babel-plugin-react-native-web": "*",
    "metro-react-native-babel-preset": "*",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0-beta",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0-beta",
    "react-native-web": ">=0.19.0",
    "storybook": "^9.0.0",
    "typescript": ">= 4.2.x"
  },
  "peerDependenciesMeta": {
    "@react-native/babel-preset": {
      "optional": true
    },
    "metro-react-native-babel-preset": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/node/index.ts",
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
