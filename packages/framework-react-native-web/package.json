{
  "name": "storybook-react-native-web-rsbuild",
  "version": "3.0.0",
  "description": "Storybook for React Native Web and Rsbuild: Develop React Native Web components with Rsbuild.",
  "keywords": [
    "storybook",
    "rsbuild",
    "rspack",
    "react-native-web",
    "react-native"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/framework-react-native-web"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./package.json": "./package.json",
    "./preset": "./dist/preset.js"
  },
  "files": ["dist/**/*", "README.md", "*.js", "*.d.ts", "!src/**/*"],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@babel/types": "^7.28.4",
    "@rsbuild/plugin-babel": "^1.0.6",
    "@storybook/react": "^10.0.0",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "storybook-builder-rsbuild": "workspace:*",
    "storybook-react-rsbuild": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.7",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "10.1.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0",
    "@babel/preset-react": "*",
    "@react-native/babel-preset": "*",
    "@rsbuild/core": "^1.5.0",
    "babel-loader": ">=9.0.0",
    "babel-plugin-react-native-web": "*",
    "metro-react-native-babel-preset": "*",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-native-web": ">=0.19.0",
    "storybook": "^10.0.0",
    "typescript": ">= 4.2.x"
  },
  "peerDependenciesMeta": {
    "@react-native/babel-preset": {
      "optional": true
    },
    "metro-react-native-babel-preset": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": ["./src/index.ts", "./src/preset.ts", "./src/node/index.ts"],
    "platform": "node"
  }
}
