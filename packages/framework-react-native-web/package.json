{
  "name": "storybook-react-native-web-rsbuild",
  "version": "3.2.0",
  "description": "Storybook for React Native Web and Rsbuild: Develop React Native components for web in isolation with Hot Reloading.",
  "keywords": [
    "storybook",
    "rsbuild",
    "rspack",
    "react-native",
    "react-native-web",
    "expo"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/framework-react-native-web"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./package.json": "./package.json",
    "./preset": "./dist/preset.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@storybook/react": "^10.1.10",
    "rsbuild-plugin-react-native-web": "workspace:*",
    "storybook-builder-rsbuild": "workspace:*",
    "storybook-react-rsbuild": "workspace:*"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.13",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.27",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native-web": "^0.19.13",
    "storybook": "10.1.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.5.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-native-web": "^0.19.0 || ^0.20.0 || ^0.21.0",
    "storybook": "^10.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/node/index.ts",
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
