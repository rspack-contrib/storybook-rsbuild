{
  "name": "storybook-addon-rslib",
  "version": "3.2.0",
  "description": "Storybook addon for loading configuration of Rslib",
  "keywords": [
    "storybook",
    "storybook-addons",
    "rsbuild",
    "rspack",
    "rslib"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/addon-rslib"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./preset": "./dist/preset.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.13",
    "@rslib/core": "^0.18.3",
    "@types/node": "^22.0.0",
    "storybook": "10.1.2",
    "storybook-builder-rsbuild": "workspace:*",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.5.0",
    "@rslib/core": ">= 0.1.1 || >= 0.2",
    "storybook-builder-rsbuild": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts"
    ],
    "platform": "node"
  }
}
