{
  "name": "storybook-react-rsbuild",
  "version": "3.0.0",
  "description": "Storybook for React and Rsbuild: Develop React components in isolation with Hot Reloading.",
  "keywords": [
    "storybook",
    "rsbuild",
    "rspack",
    "react"
  ],
  "homepage": "https://storybook.rsbuild.rs",
  "bugs": {
    "url": "https://github.com/rspack-contrib/storybook-rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rspack-contrib/storybook-rsbuild",
    "directory": "packages/framework-react"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./loaders/react-docgen-loader": {
      "types": "./dist/loaders/react-docgen-loader.d.ts",
      "default": "./dist/loaders/react-docgen-loader.js"
    },
    "./node": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./package.json": "./package.json",
    "./preset": "./dist/preset.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "scripts": {
    "build": "pnpm run prep --production",
    "check": "jiti ../../scripts/check/check-package.ts",
    "prep": "jiti ../../scripts/build/build-package.ts",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.3.0",
    "@storybook/react": "^10.0.0",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "find-up": "^5.0.0",
    "magic-string": "^0.30.21",
    "react-docgen": "^8.0.2",
    "react-docgen-typescript": "^2.4.0",
    "resolve": "^1.22.11",
    "storybook-builder-rsbuild": "workspace:*",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.7",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.26",
    "@types/resolve": "^1.20.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "storybook": "10.1.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.5.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0-beta",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0-beta",
    "storybook": "^10.1.0",
    "typescript": ">= 4.2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/node/index.ts",
      "./src/index.ts",
      "./src/preset.ts",
      "./src/loaders/react-docgen-loader.ts"
    ],
    "platform": "node"
  }
}
